{
  "name": "mern-fullstack",
  "homepage": "https://DimaShumanskiy.github.io/MERN-Fullstack_Changing_links",
  "version": "1.0.0",
  "description": "Mern stak",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\"  \"npm run client\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "mern",
    "react"
  ],
  "author": "Dima Shumanskiy <dimaihfn@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "gh-pages": "^3.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.3",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15"
  }
}
